// äº§å“åˆ—è¡¨é¡µé¢ç»„ä»¶
@Entry
@Component
struct ProductList {
  // æ’åºå­—æ®µç´¢å¼•
  private select: number  = 0;
  // æ’åºå­—æ®µé€‰é¡¹
  private paixu: string[] = ['åç§°', 'ä»·æ ¼', 'è¯„åˆ†'];
  private paixu1: string[] = ['å¢åº', 'é™åº'];
  // å½“å‰æ’åºå­—æ®µ
  @State private sortField: 'name' | 'price' | 'rating' = 'name';
  @State private sortOrder: 'asc' | 'desc' = 'asc';
  // æœç´¢è¾“å…¥å€¼
  @State v: string = '';
  // æœç´¢å…³é”®è¯
  @State private searchTerm: string = '';
  @State private products: ProductItem[] =  [...products];
  // åˆ›å»ºç‹¬ç«‹å‰¯æœ¬


  // è·å–è¿‡æ»¤å’Œæ’åºåçš„äº§å“åˆ—è¡¨
  getFilteredAndSortedProducts(): ProductItem[] {

    let result = [...this.products];

    // æ ¹æ®æœç´¢è¯è¿‡æ»¤äº§å“
    if (this.searchTerm?.trim()) {
      const searchTermLower = this.searchTerm.toLowerCase();
      result = result.filter(item =>
      item.name.toLowerCase().includes(searchTermLower)
      );
    }

    // å¯¹äº§å“è¿›è¡Œæ’åº
    result.sort((a, b) => {
      let comparison = 0;

      // æ ¹æ®æ’åºå­—æ®µè·å–æ¯”è¾ƒå€¼
      switch (this.sortField) {
        case 'name':
          comparison = a.name.localeCompare(b.name);
          break;
        case 'price':
          comparison = a.price - b.price;
          break;
        case 'rating':
          comparison = a.rating - b.rating;
          break;
        default:
          return 0;
      }

      // æ ¹æ®æ’åºé¡ºåºè°ƒæ•´ç»“æœ
      return this.sortOrder === 'asc' ? comparison : -comparison;
    });

    return result;
  }

  build() {
    Column() {
      Row(){
        Image($r('app.media.ic_back'))
          .width(24)
          .height(24)
          .margin({ top: 10, left: 20 })
          .onClick(() => {
            router.back();  // è¿”å›åˆ°ä¸Šä¸€é¡µ
          })
      }.alignSelf(ItemAlign.Start)
      // æœç´¢
      Search({ value: this.searchTerm, placeholder: "æœç´¢å•†å“..." })
              .onChange((value: string) => {
                this.searchTerm = value;
              })
              .width('90%')
              .margin({ top: 16, bottom: 10 })
              .backgroundColor('#f5f5f5')
      // æ’åºæ§åˆ¶æŒ‰é’®
      Row() {
        Button("æ’åº: " + (this.paixu[this.sortField === 'name' ? 0 : this.select === 1 ? 1 : 2]))
          .margin(20)
          .onClick(() => {
            const currentFieldIndex = this.sortField === 'name' ? 0 : this.sortField === 'price' ? 1 : 2;
            this.getUIContext().showTextPickerDialog({
              range: this.paixu,
              selected: currentFieldIndex,
              defaultPickerItemHeight: 40,
              onAccept: (value: TextPickerResult) => {
                const selectedIndex = Array.isArray(value.index) ? value.index[0] : value.index;
                this.select = selectedIndex === 0 ?0 : selectedIndex === 1 ? 1: 2;
                this.sortField = ['name', 'price', 'rating'][selectedIndex] as 'name' | 'price' | 'rating';
                console.info("Selected sort field: " + this.sortField);
              }
            });
          })

        // æ’åºé¡ºåºé€‰æ‹©æŒ‰é’®
        Button("ä»·æ ¼: " + (this.paixu1[this.sortOrder === 'asc' ? 0 : 1]))
          .margin(20)
          .onClick(() => {
            const currentOrderIndex = this.sortOrder === 'asc' ? 0 : 1;
            this.getUIContext().showTextPickerDialog({
              range: this.paixu1,
              selected: currentOrderIndex,
              defaultPickerItemHeight: 40,
              onAccept: (value: TextPickerResult) => {

                const selectedIndex = Array.isArray(value.index) ? value.index[0] : value.index;
                this.sortOrder = selectedIndex === 0 ? 'asc' : 'desc';
                console.info("Selected sort order: " + this.sortOrder);
              },

            });
          })
      }
      .justifyContent(FlexAlign.Center)

      // äº§å“åˆ—è¡¨
      List({space:10}){
        ForEach(this.getFilteredAndSortedProducts(), (item: ProductItem) => {
          ListItem() {
            Row(){
              Image(item.image)
                .width(120)
                .height(120)
                .margin({right:20})
                .backgroundColor("#eee")
                .borderRadius(8)
              Column(){
                Text(item.name)
                  .fontSize(18)
                  .margin({bottom:8})
                  .maxLines(1)
                  .textOverflow({overflow:TextOverflow.Ellipsis})
                Text(`${item.price}å…ƒ`)
                  .fontColor('#ff5722')
                  .fontSize(18)
                  .margin({bottom:8})
                Row(){
                  Text(`${item.rating}æ˜Ÿ`)
                    .fontColor('#666')
                  // åŠ å…¥è´­ç‰©è½¦æŒ‰é’®
                  Button("åŠ å…¥è´­ç‰©è½¦")
                    .width("70%")
                    .height(32)
                    .fontSize(14)
                    .type(ButtonType.Capsule)
                    .backgroundColor( '#ff5722')
                    .fontColor(Color.White)
                      // ç‚¹å‡»è·³è½¬åˆ°è¯¦æƒ…é¡µ
                    .onClick(() => {
                      this.getUIContext().getRouter().pushUrl({
                        url: "pages/zonghezuoye/ProductDetailPage",
                        params: {
                          id: item.id,
                          name: item.name,
                          price: item.price,
                          rating: item.rating,
                          productId: item.id
                        }
                      });
                    })

                }
                .justifyContent(FlexAlign.SpaceBetween)
                .width('100%')
              }
              .layoutWeight(1)

            }
            .onClick(() => {
              // ç‚¹å‡»è·³è½¬åˆ°è¯¦æƒ…é¡µ
              this.getUIContext().getRouter().pushUrl({
                url: "pages/zonghezuoye/ProductDetailPage",
                params: {
                  id: item.id,
                  name: item.name,
                  price: item.price,
                  rating: item.rating,
                  productId: item.id
                }
              });
            })
            .width('100%')
            .padding(12)
          }
          .width('100%')
          .border({width:1,color:'#f0f0f0'})
        },(item:ProductItem)=>item.id.toString())
      }
      .width('100%')
      .height('100%')
      .backgroundColor(Color.White)

    }
  }
}
